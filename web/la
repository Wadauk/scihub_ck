		function colorMenu() {
			var position = $(window).scrollTop() + screen.height - $('#menu').height();
			var targets = ['top', 'about', 'principlelist', 'donate', 'joinvk'];
			var backgrounds = ['black', 'black', '#ccc', '#ccc', '#d6504c'];
			var colors = ['white', 'white', 'black', 'black', 'white'];
			if (position < screen.height) {
				$('#up').hide();
				$('#share-vertical').hide();
			}
			else {
				$('#up').show();
				$('#share-vertical').show();
			}
			var shareV = position - $('#share').offset().top;
			if (shareV < screen.height * 0.8
				&& shareV > 0)
				$('#share-vertical').hide();
			for (var i = 0; i < 5; i++)
				if (position >= $('#' + targets[i]).offset().top)
				{
					$('#menu').css('background', backgrounds[i]);
					$('#menu ul').css('color', colors[i]);
					$('#up').css('color', colors[i]);
				}
		}
		function go(id, o) { if (typeof(o)==='undefined') o = 0; $('html,body').animate({scrollTop: $('#' + id).offset().top + o}, 500); }
	</script>
